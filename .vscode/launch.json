{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Module",
            "type": "python",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "main:server_app",
                "--reload"
            ],
            "env": {
                "LOGLEVEL": "DEBUG",
                "PREFIX": "dev-"
            },
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/app"
        }
    ]
}